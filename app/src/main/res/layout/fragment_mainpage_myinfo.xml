<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/myinfo_background"
            android:layout_width="match_parent"
            android:layout_height="337dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/myinfo_background" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="0dp"
            android:layout_height="120dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/stepbutton"
            app:layout_constraintEnd_toStartOf="@+id/guideline83"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/guideline77"
            app:srcCompat="@drawable/friend_item_button_already" />

        <ImageView
            android:id="@+id/myinfo_profile_photo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="56dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView6"
            app:layout_constraintEnd_toEndOf="@+id/imageView6"
            app:layout_constraintStart_toStartOf="@+id/imageView6"
            app:srcCompat="@drawable/profile_photo_big" />

        <TextView
            android:id="@+id/user_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="葫芦"
            android:textColor="#111111"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/myinfo_profile_photo"
            app:layout_constraintStart_toStartOf="@+id/myinfo_profile_photo"
            app:layout_constraintTop_toBottomOf="@+id/myinfo_profile_photo" />

        <ImageView
            android:id="@+id/camera_icon"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginEnd="18dp"
            android:layout_marginBottom="18dp"
            app:layout_constraintBottom_toBottomOf="@+id/myinfo_profile_photo"
            app:layout_constraintEnd_toEndOf="@+id/myinfo_profile_photo"
            app:srcCompat="@mipmap/camera_icon" />

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/user_name"
            app:layout_constraintStart_toEndOf="@+id/user_name"
            app:layout_constraintTop_toBottomOf="@+id/myinfo_profile_photo"
            app:srcCompat="@mipmap/message" />

        <ImageView
            android:id="@+id/bookbutton"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/bg_shadow"
            android:contentDescription="@string/background1"
            app:layout_constraintStart_toStartOf="@+id/guideline77"
            app:layout_constraintTop_toTopOf="@+id/stepbutton" />

        <ImageView
            android:id="@+id/mainpage_myinfo_settings_btn"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/bg_shadow"
            android:contentDescription="TODO"
            app:layout_constraintEnd_toStartOf="@+id/guideline83"
            app:layout_constraintTop_toTopOf="@+id/infobutton" />

        <ImageView
            android:id="@+id/infobutton"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/bg_shadow"
            app:layout_constraintBottom_toTopOf="@+id/guideline99"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/fg_button"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/bg_shadow"
            app:layout_constraintStart_toStartOf="@+id/guideline77"
            app:layout_constraintTop_toTopOf="@+id/infobutton" />

        <ImageView
            android:id="@+id/mainpage_myinfo_friends_btn"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginEnd="44dp"
            android:background="@drawable/bg_shadow"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/stepbutton" />

        <ImageView
            android:id="@+id/stepbutton"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/bg_shadow"
            app:layout_constraintBottom_toTopOf="@+id/infobutton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/stepbutton"
            app:layout_constraintEnd_toEndOf="@+id/stepbutton"
            app:layout_constraintStart_toStartOf="@+id/stepbutton"
            app:layout_constraintTop_toTopOf="@+id/stepbutton"
            app:layout_constraintVertical_bias="0.40"
            app:srcCompat="@mipmap/footstep" />

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="35dp"
            android:layout_height="35dp"
            app:layout_constraintBottom_toBottomOf="@+id/mainpage_myinfo_settings_btn"
            app:layout_constraintEnd_toEndOf="@+id/mainpage_myinfo_settings_btn"
            app:layout_constraintStart_toStartOf="@+id/mainpage_myinfo_settings_btn"
            app:layout_constraintTop_toTopOf="@+id/mainpage_myinfo_settings_btn"
            app:layout_constraintVertical_bias="0.41"
            app:srcCompat="@mipmap/myinfo_setting" />

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/mainpage_myinfo_friends_btn"
            app:layout_constraintEnd_toEndOf="@+id/mainpage_myinfo_friends_btn"
            app:layout_constraintStart_toStartOf="@+id/mainpage_myinfo_friends_btn"
            app:layout_constraintTop_toTopOf="@+id/mainpage_myinfo_friends_btn"
            app:layout_constraintVertical_bias="0.40"
            app:srcCompat="@mipmap/myinfo_friend" />

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            app:layout_constraintBottom_toBottomOf="@+id/bookbutton"
            app:layout_constraintEnd_toEndOf="@+id/bookbutton"
            app:layout_constraintStart_toStartOf="@+id/bookbutton"
            app:layout_constraintTop_toTopOf="@+id/bookbutton"
            app:layout_constraintVertical_bias="0.40"
            app:srcCompat="@mipmap/book_icon" />

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/fg_button"
            app:layout_constraintEnd_toEndOf="@+id/fg_button"
            app:layout_constraintStart_toStartOf="@+id/fg_button"
            app:layout_constraintTop_toTopOf="@+id/fg_button"
            app:layout_constraintVertical_bias="0.4"
            app:srcCompat="@mipmap/myinfo_mytujian" />

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginStart="19dp"
            android:layout_marginEnd="22dp"
            app:layout_constraintBottom_toBottomOf="@+id/infobutton"
            app:layout_constraintEnd_toEndOf="@+id/infobutton"
            app:layout_constraintStart_toStartOf="@+id/infobutton"
            app:layout_constraintTop_toTopOf="@+id/infobutton"
            app:layout_constraintVertical_bias="0.41000003"
            app:srcCompat="@mipmap/myinfo_information" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的预约"
            app:layout_constraintEnd_toEndOf="@+id/imageView18"
            app:layout_constraintStart_toStartOf="@+id/imageView18"
            app:layout_constraintTop_toBottomOf="@+id/imageView18" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="足迹"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView"
            app:layout_constraintEnd_toEndOf="@+id/imageView15"
            app:layout_constraintStart_toStartOf="@+id/imageView15" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="伙伴"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView2"
            app:layout_constraintEnd_toEndOf="@+id/imageView17"
            app:layout_constraintStart_toStartOf="@+id/imageView17" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="11dp"
            android:text="我的图鉴"
            app:layout_constraintEnd_toEndOf="@+id/imageView19"
            app:layout_constraintHorizontal_bias="0.625"
            app:layout_constraintStart_toStartOf="@+id/imageView19"
            app:layout_constraintTop_toBottomOf="@+id/imageView19" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="场馆信息"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView4"
            app:layout_constraintEnd_toEndOf="@+id/imageView20"
            app:layout_constraintStart_toStartOf="@+id/imageView20" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView5"
            app:layout_constraintEnd_toEndOf="@+id/imageView16"
            app:layout_constraintStart_toStartOf="@+id/imageView16" />

        <ImageView
            android:id="@+id/imageView24"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline83"
            app:layout_constraintTop_toTopOf="@+id/guideline84"
            app:srcCompat="@mipmap/myinfo_camera" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="来过国家博物馆3次"
            app:layout_constraintEnd_toEndOf="@+id/imageView6"
            app:layout_constraintStart_toStartOf="@+id/imageView6"
            app:layout_constraintTop_toBottomOf="@+id/user_name" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline75"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />

        <androidx.constraintlayout.widget.Barrier
            android:id="@+id/barrier"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:barrierDirection="bottom"
            app:constraint_referenced_ids="myinfo_background"
            tools:layout_editor_absoluteY="731dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline77"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.1" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline83"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.9" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline84"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.02" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline86"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline99"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_end="70dp" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
